{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": "single"
        },
        "trailing-comma": false,
        "no-angle-bracket-type-assertion": false,
        "whitespace": [
            false,
            "check-type-cast"
        ],
        "arrow-parens": false,
        "member-access": [
            true,
            "no-public"
        ],
        "object-literal-shorthand": false
    },
    "rulesDirectory": []
}